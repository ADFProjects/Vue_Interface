\n          <template v-slot:default=\"props\">\n            <v-row style=\"width: 1025px;\">\n              <v-col\n                v-for=\"item in props.items\"\n                :key=\"item.name\"\n                cols=\"12\"\n                sm=\"6\"\n                md=\"4\"\n                lg=\"3\"\n              >\n                <v-card>\n                  <v-card-title class=\"subheading font-weight-bold\">\n                    {{ item.name }}\n                  </v-card-title>\n\n                  <v-divider></v-divider>\n\n                  <v-list dense>\n                    <v-list-item\n                      v-for=\"(key, index) in filteredKeys\"\n                      :key=\"index\"\n                    >\n                      <v-list-item-content\n                        :class=\"{ 'light-green lighten-5': sortBy === key }\"\n                      >\n                        {{ key }}:\n                      </v-list-item-content>\n                      <v-list-item-content\n                        class=\"align-end\"\n                        :class=\"{ 'light-green lighten-5': sortBy === key }\"\n                      >\n                        {{ item[key.toLowerCase()] }}\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-list>\n                </v-card>\n              </v-col>\n            </v-row>\n          </template>\n\n          <template v-slot:footer>\n            <v-row class=\"mt-2\" align=\"center\" justify=\"center\" style=\"width: 1009px; padding: 10px;\">\n              <span class=\"grey--text\"> المعاملات لكل صفحة</span>\n              <v-menu offset-y>\n                <template v-slot:activator=\"{ on, attrs }\">\n                  <v-btn\n                    dark\n                    text\n                    color=\"green\"\n                    class=\"ml-2\"\n                    v-bind=\"attrs\"\n                    v-on=\"on\"\n                  >\n                    {{ itemsPerPage }}\n                    <v-icon>mdi-chevron-down</v-icon>\n                  </v-btn>\n                </template>\n                <v-list>\n                  <v-list-item\n                    v-for=\"(number, index) in itemsPerPageArray\"\n                    :key=\"index\"\n                    @click=\"updateItemsPerPage(number)\"\n                  >\n                    <v-list-item-title>{{ number }}</v-list-item-title>\n                  </v-list-item>\n                </v-list>\n              </v-menu>\n\n              <v-spacer></v-spacer>\n\n              <span\n                class=\"mr-4\n                grey--text\"\n                style=\"padding:10px;\"\n              >\n                الصفحة من {{ page }} إلى {{ numberOfPages }}\n              </span>\n              \n              <v-btn\n                fab\n                dark\n                color=\"green darken-3\"\n                class=\"mr-1\"\n                @click=\"formerPage\"\n                style=\"margin-left:10px;\"\n              >\n                <v-icon>mdi-chevron-right</v-icon>\n              </v-btn>\n              \n              <v-btn\n                fab\n                dark\n                color=\"green darken-3\"\n                class=\"ml-1\"\n                @click=\"nextPage\"\n              >\n                <v-icon>mdi-chevron-left</v-icon>\n              </v-btn>\n            </v-row>\n          </template>\n        </v-data-iterator>\n      </v-container>\n    </v-app>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: () => {\n    return {\n      itemsPerPageArray: [4, 8, 12],\n      search: \"\",\n      filter: {},\n      sortDesc: false,\n      page: 1,\n      itemsPerPage: 4,\n      sortBy: \"name\",\n      keys: [\n        \"رقم المعاملة\",\n        \"التاريخ\",\n        \"الموضوع\",\n        \"درجة الأهمية\",\n        \"درجة السرية\",\n        \"نوع الخطاب\",\n        \"المرفقات\",\n        \"الملاحظات\",\n      ],\n      it