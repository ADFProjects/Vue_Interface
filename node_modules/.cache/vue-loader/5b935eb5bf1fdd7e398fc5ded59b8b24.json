 id=\"inspire\">\r\n    <v-container fluid>\r\n      \r\n      <v-data-iterator\r\n        :items=\"items\"\r\n        :items-per-page.sync=\"itemsPerPage\"\r\n        :page.sync=\"page\"\r\n        :search=\"search\"\r\n        :sort-by=\"sortBy.toLowerCase()\"\r\n        :sort-desc=\"sortDesc\"\r\n        hide-default-footer\r\n      >\r\n        <template v-slot:header>\r\n          <v-toolbar\r\n            dark\r\n            color=\"#252123; darken-3\"\r\n            class=\"mb-1\"\r\n          >\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              clearable\r\n              flat\r\n              solo-inverted\r\n              hide-details\r\n              prepend-inner-icon=\"mdi-magnify\"\r\n              label=\"البحث باستخدام رقم المعاملة\"\r\n            ></v-text-field>\r\n            <template v-if=\"$vuetify.breakpoint.mdAndUp\">\r\n              <v-spacer></v-spacer>\r\n              <v-select\r\n                v-model=\"sortBy\"\r\n                flat\r\n                solo-inverted\r\n                hide-details\r\n                :items=\"keys\"\r\n                prepend-inner-icon=\"mdi-magnify\"\r\n                label=\"فزر باستخدام\"\r\n              ></v-select>\r\n              <v-spacer></v-spacer>\r\n              <v-btn-toggle\r\n                v-model=\"sortDesc\"\r\n                mandatory\r\n              >\r\n                <v-btn\r\n                  large\r\n                  depressed\r\n                  color=\"blue\"\r\n                  :value=\"false\"\r\n                >\r\n                  <v-icon>mdi-arrow-up</v-icon>\r\n                </v-btn>\r\n                <v-btn\r\n                  large\r\n                  depressed\r\n                  color=\"blue\"\r\n                  :value=\"true\"\r\n                >\r\n                  <v-icon>mdi-arrow-down</v-icon>\r\n                </v-btn>\r\n              </v-btn-toggle>\r\n            </template>\r\n          </v-toolbar>\r\n        </template>\r\n  \r\n        <template v-slot:default=\"props\">\r\n          <v-row>\r\n            <v-col\r\n              v-for=\"item in props.items\"\r\n              :key=\"item.name\"\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              md=\"4\"\r\n              lg=\"3\"\r\n            >\r\n              <v-card>\r\n                <v-card-title class=\"subheading font-weight-bold\">\r\n                  {{ item.name }}\r\n                </v-card-title>\r\n  \r\n                <v-divider></v-divider>\r\n  \r\n                <v-list dense>\r\n                  <v-list-item\r\n                    v-for=\"(key, index) in filteredKeys\"\r\n                    :key=\"index\"\r\n                  >\r\n                    <v-list-item-content :class=\"{ 'blue--text': sortBy === key }\">\r\n                      {{ key }}:\r\n                    </v-list-item-content>\r\n                    <v-list-item-content\r\n                      class=\"align-end\"\r\n                      :class=\"{ 'blue--text': sortBy === key }\"\r\n                    >\r\n                      {{ item[key.toLowerCase()] }}\r\n                    </v-list-item-content>\r\n                  </v-list-item>\r\n                </v-list>\r\n              </v-card>\r\n            </v-col>\r\n          </v-row>\r\n        </template>\r\n  \r\n        <template v-slot:footer>\r\n          <v-row\r\n            class=\"mt-2\"\r\n            align=\"center\"\r\n            justify=\"center\"\r\n          >\r\n            <span class=\"grey--text\"> المعاملات لكل صفحة</span>\r\n            <v-menu offset-y>\r\n              <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-btn\r\n                  dark\r\n                  text\r\n                  color=\"primary\"\r\n                  class=\"ml-2\"\r\n                  v-bind=\"attrs\"\r\n                  v-on=\"on\"\r\n                >\r\n                  {{ itemsPerPage }}\r\n                  <v-icon>mdi-chevron-down</v-icon>\r\n                </v-btn>\r\n              </template>\r\n              <v-list>\r\n                <v-list-item\r\n                  v-for=\"(number, index) in itemsPerPageArray\"\r\n                  :key=\"index\"\r\n                  @click=\"updateItemsPerPage(number)\"\r\n                >\r\n                  <v-list-item-title>{{ number }}</v-list-item-title>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-menu>\r\n  \r\n            <v-spacer></v-spacer>\r\n  \r\n            <span\r\n              class=\"mr-4\r\n              grey--text\"\r\n            >\r\n              الصفحة {{ page }} من {{ numberOfPages }}\r\n            </span>\r\n            <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"mr-1\"\r\n              @click=\"formerPage\"\r\n            >\r\n              <v-icon>mdi-chevron-left</v-icon>\r\n            </v-btn>\r\n            <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"ml-1\"\r\n              @click=\"nextPage\"\r\n            >\r\n              <v-icon>mdi-chevron-right</v-icon>\r\n            </v-btn>\r\n          </v-row>\r\n        </template>\r\n      </v-data-iterator>\r\n    </v-container>\r\n  </v-app>\r\n</div>\r\n\r\n<script>\r\nexport default {\r\n    data: () => ({\r\n el: '#app',\r\n  vuetify: new Vuetify(),\r\n  data () {\r\n    return {\r\n      itemsPerPageArray: [4, 8, 12],\r\n      search: '',\r\n      filter: {},\r\n      sortDesc: false,\r\n      page: 1,\r\n      itemsPerPage: 4,\r\n      sortBy: 'name',\r\n      keys: [\r\n        'رقم المعاملة',\r\n        'التاريخ',\r\n        'الموضوع',\r\n        'درجة الأهمية',\r\n        'درجة السرية',\r\n        'نوع الخطاب',\r\n        'المرفقات',\r\n        'الملاحظات',\r\n      ],\r\n      items: [\r\n        {\r\n          name: 'رقم المعاملة #1235',\r\n          ا