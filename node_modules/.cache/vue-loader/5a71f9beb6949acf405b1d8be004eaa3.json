Content":["<template class=\"my-application\">\n  <div id=\"app\" class=\"d-flex justify-center my-application\">\n    <v-app id=\"inspire\" class=\"my-application addBackground\">\n      <v-main class=\"my-application\">\n        <v-container class=\"my-application\">\n          <v-app-bar\n            elevation=\"20\"\n            style=\"border-radius: 4px; opacity: 0.9 !important\"\n            width=\"1160\"\n            color=\"#28714e\"\n            dark\n            class=\"mb-1 my-application\"\n          >\n            <v-tooltip bottom>\n              <template #activator=\"{ on }\">\n                <v-img\n                  v-on=\"on\"\n                  src=\"~@/assets/Permission.png\"\n                  alt=\"InboundImage\"\n                  max-height=\"60\"\n                  max-width=\"43\"\n                ></v-img>\n              </template>\n              <span class=\"my-application\">الصلاحيات</span>\n            </v-tooltip>\n\n            <v-tooltip bottom>\n              <template #activator=\"{ on }\">\n                <v-text-field\n                  v-on=\"on\"\n                  v-model=\"search\"\n                  clearable\n                  flat\n                  solo-inverted\n                  hide-details\n                  prepend-inner-icon=\"mdi-magnify\"\n                  label=\"البحث\"\n                  class=\"mx-4 my-application\"\n                  style=\"font-size: 16px; font-weight: bold\"\n                ></v-text-field>\n              </template>\n              <span class=\"my-application\"\n                >البحث بإستخدام جميع خانات المعاملة</span\n              >\n            </v-tooltip>\n\n            <template v-if=\"$vuetify.breakpoint.mdAndUp\">\n              <v-btn-toggle v-model=\"sortDesc\" mandatory>\n                <v-tooltip bottom>\n                  <template #activator=\"{ on }\">\n                    <v-btn\n                      v-on=\"on\"\n                      large\n                      depressed\n                      color=\"#ffffff\"\n                      :value=\"true\"\n                    >\n                      <v-icon style=\"color: #28714e\">mdi-arrow-down</v-icon>\n                    </v-btn>\n                  </template>\n                  <span class=\"my-application\"\n                    >ترتيب المعاملات من الأحدث إلى الأقدم</span\n                  >\n                </v-tooltip>\n                <v-tooltip bottom>\n                  <template #activator=\"{ on }\">\n                    <v-btn\n                      v-on=\"on\"\n                      large\n                      depressed\n                      color=\"#ffffff\"\n                      :value=\"false\"\n                    >\n                      <v-icon style=\"color: #28714e\">mdi-arrow-up</v-icon>\n                    </v-btn>\n                  </template>\n                  <span class=\"my-application\"\n                    >ترتيب المعاملات من الأقدم إلى الأحدث</span\n                  >\n                </v-tooltip>\n              </v-btn-toggle>\n            </template>\n          </v-app-bar>\n\n          <v-card>\n            <v-form\n              class=\"my-application elevation-10\"\n              ref=\"form\"\n              v-model=\"valid\"\n              lazy-validations\n            >\n              <v-container>\n                <v-card>\n                  <v-card-title class=\"grey--text text--darken-2\">\n                    الصلاحيات :</v-card-title\n                  >\n                  <v-container>\n                    <v-radio-group v-model=\"rowType\" @change=\"fawziway()\">\n                      <v-row>\n                        <v-col v-for=\"n in sendway\" :key=\"n\">\n                          <v-radio\n                            :label=\"n\"\n                            :value=\"n\"\n                            color=\"#28714e\"\n                          ></v-radio>\n                        </v-col>\n                      </v-row>\n                    </v-radio-group>\n                  </v-container>\n                </v-card>\n              </v-container>\n\n              <v-container v-show=\"spoToggle\">\n                <v-card>\n                  <v-card-title class=\"grey--text text--darken-2\">\n                    الإدارة / القسم :</v-card-title\n                  >\n                  <v-container>\n                    <v-row>\n                      <v-col>\n                        <v-autocomplete\n                          @click=\"initialize\"\n                          label=\"الإدارة\"\n                          v-model=\"autoCompleteInput\"\n                          :items=\"items\"\n                          item-text=\"name\"\n                          :item-value=\"(item) => item\"\n                          @input=\"makeRequest\"\n                          clearable\n                          outlined\n                        >\n                        </v-autocomplete>\n                      </v-col>\n                      <v-col>\n                        <v-autocomplete\n                          label=\"القسم\"\n                          v-model=\"autoCompleteInput\"\n                          :items=\"items\"\n                          item-text=\"email\"\n                          :item-value=\"(item) => item\"\n                          @input=\"makeRequest\"\n                          clearable\n                          outlined\n                        >\n                        </v-autocomplete>\n                      </v-col>\n\n                      <!-- <v-radio-group v-model=\"rowSpo\" @change=\"spo($event)\">\n                      <v-row>\n                        <v-col v-for=\"n in sendwaySPO\" :key=\"n\">\n                          <v-radio\n                            :label=\"n\"\n                            :value=\"n\"\n                            color=\"#28714e\"\n                          ></v-radio>\n                        </v-col>\n                      </v-row>\n                    </v-radio-group> -->\n                    </v-row>\n                  </v-container>\n                </v-card>\n              </v-container>\n              <v-container v-show=\"commpanyToggle\">\n                <v-card>\n                  <v-card-title class=\"grey--text text--darken-2\">\n                    الإدارة / القسم :</v-card-title\n                  >\n                  <v-container>\n                    <v-row>\n                      <v-col>\n                        <v-autocomplete\n                          label=\"الإدارة\"\n                          v-model=\"autoCompleteInput\"\n                          :items=\"items\"\n                          item-text=\"name\"\n                          :item-value=\"(item) => item\"\n                          @input=\"makeRequest\"\n                          clearable\n                          outlined\n                        >\n                        </v-autocomplete>\n                      </v-col>\n                      <v-col>\n                        <v-autocomplete\n                          label=\"القسم\"\n                          v-model=\"autoCompleteInput\"\n                          :items=\"items\"\n                          item-text=\"email\"\n                          :item-value=\"(item) => item\"\n                          @input=\"makeRequest\"\n                          clearable\n                          outlined\n                        >\n                        </v-autocomplete>\n                      </v-col>\n                      <!-- <v-radio-group\n                      v-model=\"rowdeliveryCo\"\n                      @change=\"deliveryCompany($event)\"\n                    >\n                      <v-row>\n                        <v-col v-for=\"n in deliveryCo\" :key=\"n\">\n                          <v-radio\n                            :label=\"n\"\n                            :value=\"n\"\n                            color=\"#28714e\"\n                          ></v-radio>\n                        </v-col>\n                      </v-row>\n                    </v-radio-group> -->\n                    </v-row>\n                  </v-container>\n                </v-card>\n              </v-container>\n\n              <v-card>\n                <v-data-table\n                  style=\"font-weight: bold; color: #595959\"\n                  :items-per-page=\"10\"\n                  :header-props=\"{ sortIcon: null }\"\n                  :search=\"search\"\n                  :headers=\"headers\"\n                  :items=\"desserts\"\n                  sort-by=\"IncidentNumber\"\n                  :sort-desc=\"sortDesc\"\n                  class=\"elevation-1 my-application\"\n                  :footer-props=\"{\n                    itemsPerPageOptions: [5, 10, 15, 25],\n                    'items-per-page-text': 'عدد الخدمات في الصفحة:',\n                    showFirstLastPage: true,\n                    pageText: 'من {0}-{1} إلى {2}',\n                    //'items-per-page-all-text': 'الكل',\n                  }\"\n                >\n                  <v-alert\n                    id=\"tdAll\"\n                    slot=\"no-results\"\n                    :value=\"true\"\n                    border=\"top\"\n                    color=\"#339966\"\n                    colored-border\n                    type=\"error\"\n                    elevation=\"6\"\n                    height=\"70\"\n                    style=\"\n                   color: #595959;\n                    margin-top: 15px;\n                    padding-top: 25px;\n                \"\n                  >\n                    <span class=\"my-application\">\n                      بحثك عن \" {{ search }} \" لم يعثر على نتائج.\n                    </span>\n                  </v-alert>\n                  <!-- <template v-slot:top>\n                    <v-spacer></v-spacer>\n                    <v-dialog v-model=\"dialog\" max-width=\"500px\">\n                      <v-card>\n                        <v-card-title>\n                          <span class=\"text-h5\">{{ formTitle }}</span>\n                        </v-card-title>\n                        <v-card-text>\n                          <v-container>\n                            <v-row>\n                              <v-col>\n                                <v-autocomplete\n                                  class=\"my-application\"\n                                  no-data-text=\"لاتوجد بيانات\"\n                                  :items=\"emply\"\n                                  label=\"صلاحيات الخدمات\"\n                                  outlined\n                                  deletable-chips\n                                  multiple\n                                  small-chips\n                                  style=\"width:400px;\"\n                                >\n                                </v-autocomplete>\n                              </v-col>\n                            </v-row>\n                          </v-container>\n                        </v-card-text>\n                        <v-card-actions>\n                          <v-spacer></v-spacer>\n                          <v-btn color=\"green darken-1\" text @click=\"save\">\n                            حفظ\n                          </v-btn>\n                          <v-btn color=\"green darken-1\" text @click=\"close\">\n                            الغاء\n                          </v-btn>\n                        </v-card-actions>\n                      </v-card>\n                    </v-dialog>\n                  </template> -->\n                  <template v-slot:[`item.actions`]=\"{ item }\">\n                    <!-- <v-icon small class=\"mr-2\" @click=\"editItem(item)\">\n                  mdi-account\n                </v-icon> -->\n                    <div id=\"checkboxDiv\">\n                      <v-row>\n                        <div>\n                          <v-col @click=\"editItem(item)\">\n                            <v-checkbox\n                              value=\"outBound\"\n                              color=\"#339966\"\n                              v-model=\"saveCheckbox.outBound\"\n                            >\n                              <template v-slot:label>\n                                <span id=\"checkboxLabel\">إضافة صادر</span>\n                              </template>\n                            </v-checkbox>\n                          </v-col>\n                        </div>\n                        <div>\n                          <v-col>\n                            <v-checkbox\n                              value=\"inBound\"\n                              color=\"#339966\"\n                              v-model=\"saveCheckbox.inBound\"\n                            >\n                              <template v-slot:label>\n                                <span id=\"checkboxLabel\">إضافة وارد</span>\n                              </template></v-checkbox\n                            >\n                          </v-col>\n                        </div>\n                        <div>\n                          <v-col>\n                            <v-checkbox\n                              value=\"outInquiry\"\n                              color=\"#339966\"\n                              v-model=\"saveCheckbox.outInquiry\"\n                            >\n                              <template v-slot:label>\n                                <span id=\"checkboxLabel\">إستعلام عن صادر</span>\n                              </template></v-checkbox\n                            >\n                          </v-col>\n                        </div>\n                        <div>\n                          <v-col>\n                            <v-checkbox\n                              value=\"inInquiry\"\n                              color=\"#339966\"\n                              v-model=\"saveCheckbox.inInquiry\"\n                            >\n                              <template v-slot:label>\n                                <span id=\"checkboxLabel\">إستعلام عن وارد</span>\n                              </template></v-checkbox\n                            >\n                          </v-col>\n                        </div>\n                        <div>\n                          <v-col>\n                            <v-checkbox\n                              value=\"outInReports\"\n                              color=\"#339966\"\n                              v-model=\"saveCheckbox.outInReports\"\n                            >\n                              <template v-slot:label>\n                                <span id=\"checkboxLabel\">تقارير الصادر</span>\n                              </template></v-checkbox\n                            >\n                          </v-col>\n                        </div>\n                      </v-row>\n                    </div>\n                  </template>\n\n                  <!-- <template v-slot:no-data>\n                    <v-btn color=\"primary\" @click=\"initialize\">\n                      Reset\n                    </v-btn>\n                  </template> -->\n                </v-data-table>\n                <v-container>\n                  <div class=\"text-center\">\n                    <v-btn\n                      rounded\n                      color=\"#3d7f5f\"\n                      dark\n                      large\n                      width=\"200\"\n                      @click=\"save\"\n                    >\n                      <h5\n                        class=\"my-10 my-application\"\n                        style=\"color: white; font-size: 14px\"\n                      >\n                        حفظ\n                      </h5>\n                    </v-btn>\n                  </div>\n                </v-container>\n              </v-card>\n            </v-form>\n          </v-card>\n        </v-container>\n      </v-main>\n    </v-app>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport VTooltip from \"v-tooltip\";\n\nVue.use(VTooltip);\nexport default {\n  data() {\n    return {\n      search: \"\",\n      sortDesc: true,\n      dialog: false,\n      valid: true,\n      saveIndex: -1,\n      desserts: [],\n      headers: [\n        {\n          text: \"اسم الموظف\",\n          align: \"center\",\n          sortable: false,\n          value: \"name\",\n        },\n        {\n          text: \"الصلاحيات\",\n          value: \"actions\",\n          sortable: false,\n          align: \"center\",\n        },\n      ],\n      saveCheckbox: {\n        outBound: \"\",\n        inBound: \"\",\n        outInquiry: \"\",\n        inInquiry: \"\",\n        outInReports: \"\",\n      },\n      rowType: \"\",\n      sendway: [\"\", \"المركز الرئيسي\", \"الفروع والمكاتب\"],\n      sendwaySPO: [\"الإدارة\", \"القسم\"],\n      deliveryCo: [\"الإدارة\", \"القسم\"],\n      spoToggle: false,\n      commpanyToggle: false,\n      rowdeliveryCo: \"\",\n      autoCompleteInput: {},\n      items: [\n        {\n          name: \"Peter Griffin\",\n          email: \"sexypeter@hotmail.com\",\n        },\n        {\n          name: \"Lois Griffin\",\n          email: \"loisgriffin@hotmail.com\",\n        },\n        {\n          name: \"Brian Griffin\",\n          email: \"coolbrian@gmail.com\",\n        },\n        {\n          name: \"Chris Griffin\",\n          email: \"chr1sgrif@hotmail.com\",\n        },\n        {\n          name: \"Meg\",\n          email: \"shutupmeg@hotmail.com\",\n        },\n      ],\n    };\n  },\n  methods: {\n    initialize() {\n      this.desserts = [\n        {\n          name: \"ريناد السبيعي\",\n          calories: 8586,\n          fat: \"renad.alsubaie@adf.gov.sa\",\n        },\n        {\n          name: \"رغد الشعلان\",\n          calories: 8460,\n          fat: \"raghad.alshaalan@adf.gov.sa\",\n        },\n      ];\n    },\n    save() {\n      if (this.saveIndex > -1) {\n        Object.assign(this.desserts[this.saveIndex], this.saveCheckbox);\n      } else {\n        this.desserts.push(this.saveCheckbox);\n      }\n      this.close();\n    },\n    makeRequest(value) {\n      console.log(\n        `hihi im not actually making a request, but look at the input:`,\n        value\n      );\n    },\n    fawziway() {\n      this.spoToggle = false;\n      this.commpanyToggle = false;\n      if (this.rowType.localeCompare(this.sendway[1]) == 0) {\n        // البريد السعودي\n        this.entitiesMerged = this.entitiesM;\n        this.spoToggle = true;\n      } else if (this.rowType.localeCompare(this.sendway[0]) == 0) {\n        // عادي\n        this.entitiesMerged = this.entities;\n      } else if (this.rowType.localeCompare(this.sendway[2]) == 0) {\n        // شركة شحن\n        this.commpanyToggle = true;\n      }\n    },\n    sendWayVerfication() {\n      if (this.rowType.localeCompare(this.sendway[1]) == 0) {\n        // البريد\n        this.requestBody.SourceType = \"4\";\n        this.requestBody.SenderType = \"SPO\";\n        this.requestBody.TotID = this.listSearch(this.to, this.entities);\n      } else if (this.rowType.localeCompare(this.sendway[0]) == 0) {\n        // صادر عادي\n        this.requestBody.SourceType = \"4\";\n        this.requestBody.SenderType = \"MADF\";\n        this.requestBody.TotID = this.listSearch(this.to, this.entities);\n      } else if (this.rowType.localeCompare(this.sendway[2]) == 0) {\n        console.log(\"شركات\");\n        this.requestBody.SourceType = \"4\";\n        this.requestBody.SenderType = \"deliveryCo\";\n      }\n\n      /*else {\n        // مراسلات\n        this.requestBody.SourceType = \"2\";\n        this.requestBody.SenderType = \"GOVT\";\n        this.requestBody.TotID = this.listSearch(this.to, this.entitiesM);\n      }*/\n    },\n    spo($event) {\n      console.log($event);\n      if ($event.localeCompare(this.sendwaySPO[0]) == 0) {\n        this.requestBody.DeliveryBy = \"spoIn\";\n      } else if ($event.localeCompare(this.sendwaySPO[1]) == 0) {\n        this.requestBody.DeliveryBy = \"spoOut\";\n      } else {\n        this.requestBody.DeliveryBy = \"spoOut\";\n      }\n    },\n    deliveryCompany($event) {\n      console.log($event);\n      if ($event.localeCompare(this.deliveryCo[0]) == 0) {\n        //smsa\n        this.requestBody.DeliveryBy = \"smsa\";\n      } else if ($event.localeCompare(this.deliveryCo[1]) == 0) {\n        //fedex\n        this.requestBody.DeliveryBy = \"fedex\";\n      }\n    },\n    registerInbound() {\n      /*\n*API ID* - *Dropdown List Name*  -  *Name in Code*  - *Name in Request*\nالجهة             13                  entities\nدرجة السرية             3            confidentiality\nدرجة الأهمية             4            importance                                                                                                                                 \nالتصنيف الموضوعي             1       objectiveClass\nالتصنيف             2                 category\nنوع الخطاب            11              correspondenceType\nنوع المرفق             6             attatchmentType\nتصنيف المرفق            12           attatchmentcategory\nامتداد المرفق             7          attatchmentExtention\n*/\n\n      this.requestBody.IOboundSubject = this.title;\n      this.requestBody.IOboundRemarks = this.remarks;\n      this.requestBody.OutboundDocNo = \"\";\n      this.requestBody.RequestDate = new Date().toLocaleString();\n      this.requestBody.OutboundHDate = this.date;\n\n      this.requestBody.ToGeha = this.to;\n\n      this.requestBody.RelatedEmail = this.email;\n      this.requestBody.RelatedName = this.senderName;\n      this.requestBody.RelatedPhone = this.mobileNumber;\n      this.requestBody.RelatedID = this.senderID;\n\n      this.requestBody.ImportanceVal = this.listSearch(\n        this.selectedImportance,\n        this.importance\n      );\n      this.requestBody.ConfidentialVal = this.listSearch(\n        this.selectedConfid,\n        this.confidentiality\n      );\n      this.requestBody.IOboundCategory = this.listSearch(\n        this.selectedObjectiveClass,\n        this.objectiveClass\n      );\n      this.requestBody.IOboundType = this.listSearch(\n        this.selectedCorrespondenceType,\n        this.correspondenceType\n      );\n      this.requestBody.IOboundClassification = this.listSearch(\n        this.selectedCategory,\n        this.category\n      );\n      this.requestBody.SelectedManager = this.listSearchDep(\n        this.dep,\n        this.departments\n      ).ManagerUserName;\n      this.requestBody.DestNumber = this.listSearchDep(\n        this.dep,\n        this.departments\n      ).DeptNo;\n      this.requestBody.SelectedManagerName = this.listSearchDep(\n        this.dep,\n        this.departments\n      ).GehaName;\n      this.sendWayVerfication();\n      this.addAttatchmentToRequest();\n    },\n  },\n};\n</script>\n\n<style lang=\"css\" scoped>\n@media print {\n  @page {\n    size: 30mm 21mm;\n    margin: 0;\n    padding: 0;\n  }\n}\n.v-radio >>> label {\n  font-family: \"Almarai\", sans-serif !important;\n  font-size: 0.9em;\n}\n.checkBoxs--text >>> label {\n  font-family: \"Almarai\", sans-serif !important;\n  font-size: 0.9em;\n  color: red;\n}\n.v-text-field >>> label {\n  font-family: \"Almarai\", sans-serif !important;\n  font-size: 0.9em;\n}\n.v-tooltip__content {\n  font-size: 14px !important;\n  opacity: 0.8 !important;\n  pointer-events: auto;\n  color: white;\n  background-color: #404040;\n}\n</style>\n<style>\n.v-text-field input {\n  font-size: 0.9em;\n}\n.inputNumber input[type=\"number\"] {\n  -moz-appearance: textfield;\n}\n.inputNumber input::-webkit-outer-spin-button,\n.inputNumber input::-webkit-inner-spin-button {\n  -webkit-appearance: none;\n}\n.custom-loader {\n  animation: loader 1s infinite;\n  display: flex;\n}\n@-moz-keyframes loader {\n  from {\n    transform: