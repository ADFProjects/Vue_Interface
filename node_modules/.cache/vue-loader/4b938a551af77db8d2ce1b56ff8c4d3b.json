>\n                  </v-col>\n                </v-row>\n              </v-container>\n              <v-container v-show=\"toggleInbound\">\n                <v-row>\n                  <v-col>\n                    <v-autocomplete\n                      color=\"#28714e\"\n                      class=\"my-application\"\n                      :loading=\"isLoadingdepartments\"\n                      :items=\"departments\"\n                      item-text=\"GehaName\"\n                      label=\"نسخة إلى\"\n                      v-model=\"toCopies\"\n                      outlined\n                      deletable-chips\n                      multiple\n                      small-chips\n                      readonly\n                    ></v-autocomplete>\n                  </v-col>\n                </v-row>\n              </v-container>\n              <v-container>\n                <v-row>\n                  <v-col>\n                    <v-text-field\n                      class=\"my-application\"\n                      color=\"#28714e\"\n                      readonly\n                      label=\"درجة الأهمية\"\n                      v-model=\"selectedImportance\"\n                      required\n                      outlined\n                    ></v-text-field>\n                  </v-col>\n                  <v-col>\n                    <v-text-field\n                      class=\"my-application\"\n                      color=\"#28714e\"\n                      readonly\n                      label=\"درجة السرية\"\n                      v-model=\"selectedConfid\"\n                      required\n                      outlined\n                    ></v-text-field>\n                  </v-col>\n                </v-row>\n              </v-container>\n              <v-container>\n                <v-row>\n                  <v-col>\n                    <v-text-field\n                      class=\"my-application\"\n                      color=\"#28714e\"\n                